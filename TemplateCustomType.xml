<?xml version="1.0" encoding="utf-8"?>
<ProducerTemplate xmlns="https://github.com/ErgodicMage/PDFProducer">
  <MetaData>
    <MetaDataProperties>
      <Version>1.7</Version>
    </MetaDataProperties>
    <MetaDataProperties>
      <MetaDataDescription>
        <Title>Example Template</Title>
        <Author>Ergodic Mage</Author>
        <Subject>Experimental Serializer</Subject>
        <Keywords>PDFProducer Template, Serializer, Experimental</Keywords>
      </MetaDataDescription>
    </MetaDataProperties>
  </MetaData>
  <Document name="Document 1">
    <Font name="arial" />
    <Page name="Page 1">
      <Description>The first page</Description>
    </Page>
    <Chapter name="Chapter 1">
      <Font name="font1" />
      <Title>Chapter Title 1</Title>
      <Description>The first chapter</Description>
      <Page name="Page 2">
        <Font name="arial" />
        <Description>The second page</Description>
      </Page>
      <ChapterSection>
        <SectionNumber>1.1</SectionNumber>
        <Title>Section 1 Title</Title>
      </ChapterSection>
    </Chapter>
  </Document>
  <Fonts>
    <Font name="font1">
      <FontName>Font 1</FontName>
      <File>Font1.ttf</File>
      <Embed>True</Embed>
    </Font>
    <Font name="arial">
      <FontName>Arial</FontName>
      <Embed>False</Embed>
    </Font>
  </Fonts>
</ProducerTemplate>